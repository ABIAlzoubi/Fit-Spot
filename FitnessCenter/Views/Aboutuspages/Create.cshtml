@model FitnessCenter.Models.Aboutuspage

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Create</h1>

<h4>Aboutuspage</h4>
<hr />
<div class="row">
